@startuml
title Diagrama de Pacotes

package "src" {
    package "assets" {
        [logo.svg]
    }

    package "components" {
        [BairrosGrid.vue]
        [BairrosManagement.vue]
        [BlogArtigo.vue]
        [PontoColetaCard.vue]
        [PontosColetaManagement.vue]
        [PostsManagement.vue]
    }

    package "db" {
